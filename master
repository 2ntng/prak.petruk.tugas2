#include <iostream>
using namespace std;

void Prime (int *);

int main()
{
	int n;
	int *pn = &n;
	cout << "Masukkan banyak angka : ";
	cin >> *pn;
	// cek(&n);
	int angka[n];
	int *pa = angka;

	for (int i = 0; i < n; i++)
	{
		cout << "Angka ke-" << i+1 << " = " ;
		cin >> *(pa+i);
		// cek(&angka[i]);
	}

}
